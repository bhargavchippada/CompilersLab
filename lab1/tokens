		enum Tokens{
			IDENTIFIER=0x100,
			INT_CONST,
			FLOAT_CONST,
			STRING_LITERAL,
			WHITESPACES,
			COMMENT,
			TYPE_SPECIFIER,
			RETURN,
			WHILE,
			FOR,
			IF,
			ELSE,
			OR_OP,
			AND_OP,
			LE_OP,
			GE_OP,
			LT_OP,
			GT_OP,
			NE_OP,
			EQ_OP,
			INC_OP,
			DEC_OP,
			NOT_OP,
			ANP_OP,
			ASGN_OP
		};

